{
  "name": "ListableUI",
  "version": "16.1.0",
  "summary": "Declarative list views for iOS apps that deploy back to iOS 14.0.",
  "homepage": "https://github.com/kyleve/Listable",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Kyle": "k@squareup.com"
  },
  "source": {
    "git": "https://github.com/kyleve/Listable.git",
    "tag": "16.1.0"
  },
  "platforms": {
    "ios": "15.0"
  },
  "swift_versions": [
    "5.9"
  ],
  "source_files": "ListableUI/Sources/**/*.{swift,h,m}",
  "resource_bundles": {
    "ListableUIResources": "ListableUI/Resources/**/*"
  },
  "weak_frameworks": "SwiftUI",
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "YES"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "ListableUI/Tests/**/*.{swift}",
      "ios": {
        "resource_bundles": {
          "ListableUITestsResources": "ListableUI/Tests/Resources/**/*.*"
        }
      },
      "frameworks": "XCTest",
      "requires_app_host": true,
      "dependencies": {
        "EnglishDictionary": [],
        "Snapshot": []
      },
      "pod_target_xcconfig": {
        "APPLICATION_EXTENSION_API_ONLY": "NO"
      }
    }
  ],
  "swift_version": "5.9"
}
